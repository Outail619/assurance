                    console.log("error");
                }
            });
        }

        function setPrice(id,nom) {

        DATA =  {'serie' : nom};
        $.ajax({
            type: "POST",
            url: "{{ path('FormulePrice')}}",
            data:DATA,
            success: function (data) {

                $("#formule").val(nom);
                $.each(data[0],function(i,ele)
                {

                    $('#'+ele.id).replaceWith('<li id="'+ele.id+'">'+'<span >'+'Prix'+'</span>'+ele.total+'</li>' );
                });
                $.each(data[1],function(k,el)
                {
                    $('#'+el.nomGarantie).prop("checked", true);

                });
            },
            error:function ()
            {
                console.log("error");
            }
        });
    }
    function changePrice(nomGarantie)
    {
        var formule=$('#formule').val();

        DATA =  {'nomGarantie' : nomGarantie,'formule' : formule};
        $.ajax({
            type: "POST",
            url: "{{ path('garantiePrice')}}",
            data:DATA,
            success: function (data) {

                $.each(data,function(i,ele)
                {

                        alert(ele.valeur);
                });

            },
            error:function ()
            {
                console.log("error");
            }
        });


    }


